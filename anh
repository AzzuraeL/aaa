#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main() {
    char in1[1000][100], in2[1000][100];
    int a, umur[1000];
    scanf("%d", &a);
    getchar();
    
    for (int i = 0; i < a; i++) {
        scanf("%s %s %d", in1[i], in2[i], &umur[i]);
    }
    
    printf("\nOriginal list\n-----------------------------------\n");
    for (int i = 0; i < a; i++) {
        printf("%-20s %-20s %3d\n", in1[i], in2[i], umur[i]);
    }

    for (int i = 0; i < a - 1; i++) {
        for (int j = i + 1; j < a; j++) {
            if (strcmp(in1[i], in1[j]) > 0) {
                char temp[100];
                strcpy(temp, in1[i]);
                strcpy(in1[i], in1[j]);
                strcpy(in1[j], temp);

                strcpy(temp, in2[i]);
                strcpy(in2[i], in2[j]);
                strcpy(in2[j], temp);

                int temp2 = umur[i];
                umur[i] = umur[j];
                umur[j] = temp2;
            }
        }
    }
    
    printf("\nAlphabetized list\n-----------------------------------\n"); 
    for (int i = 0; i < a; i++) {
        printf("%s %-20s %3d\n", in1[i], in2[i], umur[i]);
    }
    return 0;
}
